{
  "name": "masses-and-springs",
  "version": "1.1.0-dev.2",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/masses-and-springs.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "MASSES_AND_SPRINGS",
    "phetLibs": [
      "griddle",
      "twixt"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simulation": true,
    "supportsOutputJS": true,
    "simFeatures": {
      "colorProfiles": [
        "default",
        "basics"
      ],
      "supportsDynamicLocale": false
    },
    "published": true,
    "screenNameKeys": [
      "MASSES_AND_SPRINGS/screen.intro",
      "MASSES_AND_SPRINGS/screen.vectors",
      "MASSES_AND_SPRINGS/screen.energy",
      "MASSES_AND_SPRINGS/screen.lab"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "rules": {
      "no-view-imported-from-model": "off"
    }
  }
}